{
  "name": "dj-set-manager-jcci",
  "version": "1.0.0",
  "description": "DJ Set List Manager for One Love Jamaica Relief - JCCI Event Management System",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "setup": "node scripts/setup.js",
    "test-api": "node scripts/test-api.js",
    "populate": "node scripts/populate-slots.js && node scripts/add-djs.js",
    "dev": "node src/index.js",
    "test": "node --test tests/*.test.js",
    "lint": "eslint src scripts",
    "event:start": "node scripts/event-start.js",
    "event:metrics": "node scripts/metrics-entry.js",
    "event:complete": "node scripts/event-complete.js",
    "event:report": "node scripts/generate-report.js",
    "dj:list": "node scripts/dj-list.js",
    "dj:confirm": "node scripts/dj-confirm.js",
    "dj:remind": "node scripts/dj-remind.js",
