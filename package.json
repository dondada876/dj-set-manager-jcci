  "homepage": "https://github.com/dbucknor/dj-set-manager-jcci#readme",
  "dependencies": {
    "airtable": "^0.12.2",
    "dotenv": "^17.2.3",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0"
  },
